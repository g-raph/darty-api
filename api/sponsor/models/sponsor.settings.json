{
  "kind": "collectionType",
  "collectionName": "sponsors",
  "info": {
    "name": "sponsor"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "url": {
      "type": "string"
    },
    "sponsorship": {
      "type": "enumeration",
      "enum": [
        "big",
        "medium",
        "small"
      ]
    }
  }
}
